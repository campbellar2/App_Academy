def perfect_square?(num)
    arr = []

    (2...num).each do |factor|
        if num % factor == 0
            arr << factor
        end
    end
    if arr.length == 1
        return true
    else
        return false
    end
end

puts perfect_square?(5)   #=> false
puts perfect_square?(12)  #=> false
puts perfect_square?(30)  #=> false
puts perfect_square?(9)   #=> true
puts perfect_square?(25)  #=> true
