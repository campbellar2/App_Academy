def doubler(numbers)
    i = 0
    
    doubled = []
    while i < numbers.length
        oldNum = numbers[i]
        newNum = oldNum * 2
        doubled << newNum

        i+= 1
    end
    return doubled

end

print doubler([1, 2, 3, 4]) # => [2, 4, 6, 8]
puts
print doubler([7, 1, 8])    # => [14, 2, 16]
